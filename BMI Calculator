weight = int(input("enter your weight in kg "))
height = float(input("enter your height in meters "))
formula = (weight / (height*height))
if formula < 18.5:  
    print("Underweight")
if 18.5 <= formula < 25:
    print("Normal")   
if 25 <= formula < 30:
    print("Overweight")
if formula >=30:
    print("Obesity")
